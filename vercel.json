{
  "functions": {
    "app/api/*/route.ts": {
      "maxDuration": 30
    },
    "app/api/*/*/route.ts": {
      "maxDuration": 30
    },
    "app/api/*/*/*/route.ts": {
      "maxDuration": 30
    },
    "app/api/*/*/*/*/route.ts": {
      "maxDuration": 30
    },
    "app/_backend/api/*/route.ts": {
      "maxDuration": 30
    },
    "app/_backend/api/*/*/route.ts": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/_backend/api/:path*",
      "destination": "/api/:path*"
    }
  ]
}